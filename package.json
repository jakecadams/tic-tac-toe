{
  "name": "ti-tac-toe",
  "scripts": {
    "js-watch": "watchify src/app.jsx -t babelify -o build/all.js -v",
    "js-build": "browserify src/app.jsx -t babelify -o build/all.js",
    "server": "python -m SimpleHTTPServer 8000",
    "start": "npm run server & npm run js-watch"
  },
  "dependencies": {
    "react": "0.13.3"
  },
  "devDependencies": {
    "babelify": "6.1.1",
    "browserify": "11.0.0",
    "watchify": "3.2.1"
  }
}
